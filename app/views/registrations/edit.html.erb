<%= render 'layouts/loggedinbody'%>
&nbsp;
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :method => :put }) do |f| %>
  <!--
  <%= devise_error_messages! %>
  -->
  <p><%= f.label :email, "EMAIL ADDRESS" %><br />
  <%= f.email_field :email , :class => "textfield_effect", :id => "on_focus_textfield"%></p>
  
  <p><%= f.label :fname, "FIRST NAME" %><br />
  <%= f.text_field :fname , :class => "textfield_effect", :id => "on_focus_textfield"%></p>
  
  <p><%= f.label :lname, "LAST NAME" %><br />
  <%= f.text_field :lname , :class => "textfield_effect", :id => "on_focus_textfield"%></p>

  <p><%= f.label :password, "PASSWORD" %><br />
  <%= f.password_field :password , :class => "textfield_effect", :id => "on_focus_textfield"%></p>

  <p><%= f.label :password_confirmation, "CONFIRM PASSWORD" %><br />
  <%= f.password_field :password_confirmation , :class => "textfield_effect", :id => "on_focus_textfield"%></p>

  <p><%= f.label :current_password, "CURRENT PASSWORD" %><br />
  <%= f.password_field :current_password , :class => "textfield_effect", :id => "on_focus_textfield"%></p>

  <p><%= f.submit "Update" %></p>
  <p><%= link_to "Cancel", dashboard_index_url %></p>
<% end %>

